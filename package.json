  "main": "index.js",
  "private": true,
  "homepage": "https://hodlstream.com",
  "dependencies": {
    "apollo-client-preset": "^1.0.4",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "force-secure-express": "^1.0.0",
    "graphiql": "^0.11.10",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.5.0",
    "highcharts": "^6.0.3",
    "ignore-styles": "^5.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jest-cli": "^22.0.3",
    "jss": "^9.4.0",
    "jss-preset-default": "^4.0.1",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.22",
    "material-ui-icons": "^1.0.0-beta.17",
    "material-ui-pickers": "^1.0.0-beta.7",
    "moment": "^2.19.3",
    "morgan": "^1.9.0",
    "raw-loader": "^0.5.1",
    "react": "^16.0.0",
    "react-adsense": "^0.0.6",
    "react-apollo": "^2.0.1",
    "react-dom": "^16.0.0",
    "react-ga": "^2.3.5",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.2.0",
    "react-ladda": "^5.0.7",
    "react-progress-button": "^5.1.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-delta": "^0.1.2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "react-scripts": "1.1.2"
  },
  "scripts": {
    "dev": "nodemon exec \"yarn build && yarn serve\"",
    "start": "react-scripts start",
    "start:server": "node server/index.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "serve": "REACT_APP_GRAPHQL=http://localhost:3000/graphql NODE_ENV=development nodemon server/index.js",
    "predeploy": "npm run build",
    "make": "docker build -t hodlstream . && npm run tag && npm run push",
    "tag": "docker tag hodlstream:latest 530472910524.dkr.ecr.us-east-1.amazonaws.com/hodlstream:latest",
    "push": "$(aws ecr get-login --no-include-email --region us-east-1) && docker push 530472910524.dkr.ecr.us-east-1.amazonaws.com/hodlstream:latest",
    "connect": "ssh -i ./hodlstream.pem ec2-user@ec2-18-208-137-23.compute-1.amazonaws.com",
    "deploy": "npm run deploy:static && npm run deploy:index",
    "deploy:static": "aws s3 sync build/ s3://hodlstream.com --exclude 'index.html'",
    "deploy:index": "aws s3 cp build/index.html s3://hodlstream.com/index.html --cache-control 'public, must-revalidate, proxy-revalidate, max-age=0'"
  }
}
