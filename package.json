{
  "name": "hodl-stream",
  "version": "0.1.0",
  "main": "index.js",
  "private": true,
  "homepage": "https://hodlstream.com",
  "dependencies": {
    "apollo-client-preset": "^1.0.4",
    "graphiql": "^0.11.10",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "highcharts": "^6.0.3",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.22",
    "material-ui-icons": "^1.0.0-beta.17",
    "material-ui-pickers": "^1.0.0-beta.7",
    "moment": "^2.19.3",
    "raw-loader": "^0.5.1",
    "react": "^16.0.0",
    "react-apollo": "^2.0.1",
    "react-dom": "^16.0.0",
    "react-ga": "^2.3.5",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "react-scripts": "1.0.16"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "REACT_APP_GA='UA-111446637-1' react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "npm run deploy:static && npm run deploy:index",
    "deploy:static": "aws s3 sync build/ s3://hodlstream.com --exclude 'index.html'",
    "deploy:index": "aws s3 cp build/index.html s3://hodlstream.com/index.html --cache-control 'public, must-revalidate, proxy-revalidate, max-age=0'"
  }
}
